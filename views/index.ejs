<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }
        table {
            width: 100%;
            border-collapse: collapse;
            margin-bottom: 20px;
        }
        th, td {
            padding: 10px;
            border: 1px solid #ddd;
            text-align: left;
        }
        th {
            background-color: #f4f4f4;
        }
        h2 {
            margin-top: 30px;
        }
    </style>
</head>
<body>
    <h1>Dashboard</h1>

    <!-- Screenings Table -->
    <h2>Child Facility Screenings</h2>
    <% if (screenings && screenings.length > 0) { %>
        <table>
            <thead>
                <tr>
                    <!-- Dynamically generate table headers -->
                    <% for (const key in screenings[0]) { %>
                        <th><%= key %></th>
                    <% } %>
                </tr>
            </thead>
            <tbody>
                <!-- Dynamically generate table data -->
                <% for (const obj of screenings) { %>
                    <tr>
                        <% for (const key in obj) { %>
                            <td><%= obj[key] %></td>
                        <% } %>
                    </tr>
                <% } %>
            </tbody>
        </table>
    <% } else { %>
        <p>No screenings data available.</p>
    <% } %>

    <!-- Child Follow-Up Table -->
    <h2>Child Facitility Follow-Up</h2>
    <% if (cFollowUp && cFollowUp.length > 0) { %>
        <table>
            <thead>
                <tr>
                    <!-- Dynamically generate table headers -->
                    <% for (const key in cFollowUp[0]) { %>
                        <th><%= key %></th>
                    <% } %>
                </tr>
            </thead>
            <tbody>
                <!-- Dynamically generate table data -->
                <% for (const obj of cFollowUp) { %>
                    <tr>
                        <% for (const key in obj) { %>
                            <td><%= obj[key] %></td>
                        <% } %>
                    </tr>
                <% } %>
            </tbody>
        </table>
    <% } else { %>
        <p>No child follow-up data available.</p>
    <% } %>
</body>
</html>